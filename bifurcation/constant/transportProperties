FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Aqui vai o modelo do fluido: Fluido Newtoniano
transportModel  Newtonian;

// Esta é a viscosidade cinemática do fluido
// Corresponde à viscosidade dinamica dividida pela massa específica
nu              [0 2 -1 0 0 0 0] 3.3e-6;

